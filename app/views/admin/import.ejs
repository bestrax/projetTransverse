<link href="/assets/css/login.css" rel="stylesheet">

<div id="login">

    <div class="head">
        Importation utilisateur
    </div>

    <div class="content">
        <form method="post" action="#" enctype="multipart/form-data">

            <% if (typeof err != 'undefined') { %>
            <div class="alert alert-danger"><%= err %></div>
            <% } %>

            <div class="form-group is-empty">
                <label for="inputPayload" class="col-md-2 control-label">Payload</label>
                <div class="col-md-10">
                    <textarea class="form-control" id="inputPayload" placeholder="Payload" type="text" name="payload"><%= typeof form.payload == 'undefined' ? '' : form.payload %></textarea>
                </div>
                <span class="material-input"></span>
            </div>

            <button type="submit" class="btn btn-success pull-right">Ajouter</button>
    </div>

</div>